{
  "name": "tsclowe",
  "version": "2.0.0",
  "private": true,
  "workspaces": ["frontend"],
  "scripts": {
    "build": "yarn --cwd frontend build && wails build",
    "dev": "wails dev -debounce 1000 2>&1 | grep -v -E 'KnownStructs:|Not found:'",
    "install": "yarn --cwd frontend install && go mod tidy && yarn build",
    "lint": "yarn --cwd frontend lint",
    "lint:tsx": "yarn --cwd frontend lint",
    "start": "yarn dev",
    "test": "yarn --cwd frontend test",
    "test:tsx": "yarn --cwd frontend test",
    "type-check": "yarn --cwd frontend type-check"
  },
  "dependencies": {}
}
